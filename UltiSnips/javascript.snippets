snippet cl
console.log($1)
endsnippet

snippet fun "function" i
function $1 ($2) {
  $0
}
endsnippet

snippet arr "arrow function"
($1) => {
  $0
}
endsnippet

snippet re "require"
const $1 = require('$1')
endsnippet

snippet im "import"
import $1 from '$1'
endsnippet

snippet us "use strict"
'use strict'
endsnippet

snippet err "throw err"
if (err) throw err
endsnippet
