#!/bin/bash

# Open current repo in stash

remote=`git remote -v | grep 'origin' | head -1 | awk '{print $2}' | cut -d@ -f2`
url=`echo "$remote" | cut -d\/ -f1 | sed "s/:7999//"`
project=`echo "$remote" | cut -d\/ -f2`
repo=`echo "$remote" | cut -d\/ -f3 | cut -d. -f1`

open "https://$url/projects/$project/repos/$repo"
