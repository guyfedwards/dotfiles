/* Xft */
Xft.lcdfilter: lcddefault
Xft*dpi: 180
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

URxvt*background: #3a3a3a
URxvt*foreground: #d0d0d0
URxvt*color0: #4e4e4e
URxvt*color1: #d68787
URxvt*color2: #5f865f
URxvt*color3: #d8af5f
URxvt*color4: #85add4
URxvt*color5: #d7afaf
URxvt*color6: #87afaf
URxvt*color7: #d0d0d0
URxvt*color8: #626262
URxvt*color9: #d75f87
URxvt*color10: #87af87
URxvt*color11: #ffd787
URxvt*color12: #add4fb
URxvt*color13: #ffafaf
URxvt*color14: #87d7d7
URxvt*color15: #e4e4e4
URxvt*cursorColor: #d0d0d0
URxvt*cursorColor2: #3a3a3a
URxvt*colorBD: #e4e4e4

URxvt.visualBell: true
URxvt.loginShell: true
URxvt.scrollBar: false
URxvt*skipScroll: true
URxvt*fading:  30
URxvt*font: xft:monaco:size=8
URxvt*boldFont: xft:monaco:size=8
URxvt*italicFont: xft:monaco:italic:size=8
/* URxvt*letterSpace: -6 */

*URxvt.perl-ext-common:	default,matcher,osc-xterm-clipboard,resize-font
URxvt*perl-lib:            /usr/lib/urxvt/perl/
URxvt.url-launcher: /usr/bin/chromium
URxvt*matcher.button:        1
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

